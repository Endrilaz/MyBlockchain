nst { address, balance } = this.state.walletInfo;\n\n        return (\n            <div className='App'>\n\n                <Navbar collapseOnSelect expand=\"lg\" bg=\"dark\" variant=\"dark\">\n                <Navbar.Brand href=\"/\">\n                    <img\n                        src={logo}\n                        width=\"30\"\n                        height=\"30\"\n                        className=\"d-inline-block align-top\"\n                        alt=\"React Bootstrap logo\"\n                    />{' '}\n                    ZIMABLUE<strong>.</strong>\n                    </Navbar.Brand>\n                <Navbar.Toggle aria-controls=\"responsive-navbar-nav\" />\n                <Navbar.Collapse className=\"justify-content-end\">\n                    <Nav>\n                    <Nav.Link href=\"/\">Home</Nav.Link>\n                    <Nav.Link href=\"/blocks\">Blocks</Nav.Link>\n                    <Nav.Link href=\"/conduct-transaction\">Make a Transaction</Nav.Link>\n                    <Nav.Link href=\"/transaction-pool\">Pool</Nav.Link>\n                    </Nav>\n                </Navbar.Collapse>\n                </Navbar>\n\n                <div className='zimablue-line-full'></div>\n\n                {/* <Image src={bg} fluid /> */}\n\n                <div className='header-txt'>\n                    <h1>ZIMABLUE<strong>.</strong></h1>\n                    <h2>\n                    ZB is a prototype project of an emulated cryptocurrency based on blockchain\n                    </h2>\n                </div>\n\n                <div className='zimablue-line-full'></div>\n\n                <Container>\n                <Row>\n                <Col md={{ span: 10, offset: 1 }}>\n\n\n                <div className='wallet-section'>\n                <div className='container'>\n                    <div className=\"text-center\">\n\n                    <div className='Card'>\n                    <Card>\n                    <Card.Header>My wallet</Card.Header>\n                    <Card.Body>\n                        <blockquote className=\"blockquote mb-0\">\n                        <p>\n                            <div className='WalletInfo'>\n                                <div>\n                                    Address\n                                    <br />\n                                    {address}\n                                </div>\n                                <div>\n                                    Balance\n                                    <br />\n                                    {balance}\n                                </div>\n                            </div>\n                        </p>\n                        </blockquote>\n                        {/* <Button variant=\"info\">Info</Button> */}\n                    </Card.Body>\n                    </Card>\n                    </div>\n\n                    </div>\n                </div>\n                </div>\n\n                </Col>\n                </Row>\n                </Container>\n\n            </div>\n        );\n    }\n}\n\nexport default App;"},"lineCount":null}},"hash":"96bbfe3f53ca6e6a1c06b24aa32de1d9","cacheData":{"env":{}}}